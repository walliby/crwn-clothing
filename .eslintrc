{
  "extends": ["wesbos"],
  "rules": {
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/no-access-state-in-setstate": 0,
    "react/state-in-constructor": 0,
    "react/jsx-props-no-spreading": 0,
    "jsx-a11y/label-has-associated-control": [
      "error",
      {
        "labelComponents": [],
        "labelAttributes": [],
        "controlComponents": [],
        "assert": "either",
        "depth": 25
      }
    ]
  }
}
